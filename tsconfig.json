{
    "version": "2.3.4",
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": false,
        "noLib": false,
        "preserveConstEnums": false,
        "suppressImplicitAnyIndexErrors": true,
        "sourceMap": true,
        "moduleResolution": "node"

    },
    "include": [
        "./tasks/**/*.ts"
    ],
    "atom": {
        "rewriteTsconfig": false
    }
}
